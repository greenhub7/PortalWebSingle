                <label class="form-check-label" for="chorioamnionitisYes">si</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="Chorioamnionitis" value="2" id="chorioamnionitisNo" @(((int?)Model?.Chorioamnionitis) == 2 ? "checked" : "") />
                <label class="form-check-label" for="chorioamnionitisNo">no</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="Chorioamnionitis" value="3" id="chorioamnionitisNc" @(((int?)Model?.Chorioamnionitis) == 3 ? "checked" : "") />
                <label class="form-check-label" for="chorioamnionitisNc">nc</label>
            </div>
        </div>
    </div>
</div>

<hr class="my-3" />
 

<!-- TRABAJO DE PARTO (5 filas como CONSULTAS ANTENATALES) -->
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label class="font-weight-bold">TRABAJO DE PARTO</label>
            <div class="table-responsive">
                <table class="table table-sm table-bordered labor-entries-table">
                    <thead class="thead-light">
                        <tr>
                            <th>Hora</th>
                            <th>Min</th>
                            <th>Posicion madre</th>
                            <th>P.A.</th>
                            <th>Pulso</th>
                            <th>Contr./10'</th>
                            <th>Dilatacion</th>
                            <th>Altura Present.</th>
                            <th>Variedad posic.</th>
                            <th>Meconio</th>
                            <th>FCF/dips</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < 5; i++)
                        {
                            <tr class="labor-entry-row">
                                <td><input type="number" class="form-control form-control-sm labor-hour" min="0" max="23" placeholder="Hora" /></td>
                                <td><input type="number" class="form-control form-control-sm labor-minute" min="0" max="59" placeholder="Min" /></td>
                                <td>
                                    <select class="form-control form-control-sm labor-position">
                                        <option value="">Seleccione</option>
                                        <option value="sentada">Sentada</option>
                                        <option value="acostada">Acostada</option>
                                        <option value="cuclillas">Cuclillas</option>
                                    </select>
                                </td>
                                <td><input type="text" class="form-control form-control-sm labor-bp" placeholder="###/##" /></td>
                                <td><input type="number" class="form-control form-control-sm labor-pulse" min="40" max="200" /></td>
                                <td><input type="number" class="form-control form-control-sm labor-contractions" min="0" max="10" /></td>
                                <td><input type="number" step="0.1" class="form-control form-control-sm labor-dilation" min="0" max="10" /></td>
                                <td><input type="text" class="form-control form-control-sm labor-height" /></td>
                                <td><input type="text" class="form-control form-control-sm labor-variety" /></td>
                                <td>
                                    <div class="form-check">
                                        <input class="form-check-input labor-meconium" type="checkbox" />
                                        <label class="form-check-label">+</label>
                                    </div>
                                </td>
                                <td><input type="text" class="form-control form-control-sm labor-fcf" /></td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<!-- ENFERMEDADES - TDP PRUEBA -->
<div class="row mt-3">
    <div class="col-md-12">
        <div class="form-group" style="background-color: #f8f9fa; padding: 10px; border-radius: 4px; border-left: 4px solid #6c757d;">
            <label class="font-weight-bold">ENFERMEDADES</label>
            <div class="ml-3">
                <label class="small font-weight-bold">TDP Prueba</label>
                <div class="row">
                    <div class="col-md-3">
                        <label class="small">Sifilis</label>
                        <div class="d-flex flex-wrap">
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthSyphilisTest" value="1" id="birthSyphilisNeg" @(((int?)Model?.BirthSyphilisTest) == 1 ? "checked" : "") />
                                <label class="form-check-label" for="birthSyphilisNeg">-</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthSyphilisTest" value="2" id="birthSyphilisPos" @(((int?)Model?.BirthSyphilisTest) == 2 ? "checked" : "") />
                                <label class="form-check-label" for="birthSyphilisPos">+</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthSyphilisTest" value="3" id="birthSyphilisNr" @(((int?)Model?.BirthSyphilisTest) == 3 ? "checked" : "") />
                                <label class="form-check-label" for="birthSyphilisNr">n/r</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthSyphilisTest" value="4" id="birthSyphilisNc" @(((int?)Model?.BirthSyphilisTest) == 4 ? "checked" : "") />
                                <label class="form-check-label" for="birthSyphilisNc">n/c</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label class="small">VIH</label>
                        <div class="d-flex flex-wrap">
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthHIVTest" value="1" id="birthHIVNeg" @(((int?)Model?.BirthHIVTest) == 1 ? "checked" : "") />
                                <label class="form-check-label" for="birthHIVNeg">-</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthHIVTest" value="2" id="birthHIVPos" @(((int?)Model?.BirthHIVTest) == 2 ? "checked" : "") />
                                <label class="form-check-label" for="birthHIVPos">+</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="BirthHIVTest" value="3" id="birthHIVNr" @(((int?)Model?.BirthHIVTest) == 3 ? "checked" : "") />
                                <label class="form-check-label" for="birthHIVNr">n/r</label>

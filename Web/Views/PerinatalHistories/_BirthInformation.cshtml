            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="Presentation" value="cephalic" id="cephalicPresentation" @(Model?.CephalicPresentation == true ? "checked" : "") />
                <label class="form-check-label" for="cephalicPresentation">cefalica</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="Presentation" value="pelvic" id="pelvicPresentation" @(Model?.PelvicPresentation == true ? "checked" : "") />
                <label class="form-check-label" for="pelvicPresentation">pelviana</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="Presentation" value="transverse" id="transverseSituation" @(Model?.TransverseSituation == true ? "checked" : "") />
                <label class="form-check-label" for="transverseSituation">transversa</label>
            </div>
        </div>
    </div>
    <div class="col-md-2">
        <label>TAMANO FETAL ACORDE</label>
        <div>
            <div class="form-check form-check-inline">
                <input class="form-check-input bool-radio" type="radio" asp-for="FetalSizeAppropriate" value="false" id="fetalSizeAppropriateNo" />
                <label class="form-check-label" for="fetalSizeAppropriateNo">no</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input bool-radio" type="radio" asp-for="FetalSizeAppropriate" value="true" id="fetalSizeAppropriateYes" />
                <label class="form-check-label" for="fetalSizeAppropriateYes">si</label>
            </div>
        </div>
    </div>
    <div class="col-md-2">
        <label>ACOMPANANTE TDP</label>
        <div class="d-flex flex-wrap">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="Companion" value="pareja" id="companionPartner" />
                <label class="form-check-label" for="companionPartner">pareja</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="Companion" value="familiar" id="companionFamily" />
                <label class="form-check-label" for="companionFamily">familiar</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="Companion" value="otro" id="companionOther" />
                <label class="form-check-label" for="companionOther">otro</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="Companion" value="ninguno" id="companionNone" />
                <label class="form-check-label" for="companionNone">ninguno</label>
            </div>
        </div>
    </div>
    <div class="col-md-2">
        <label>ACOMPANANTE P</label>
        <div class="d-flex flex-wrap">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="CompanionP" value="pareja" id="companionPPartner" />
                <label class="form-check-label" for="companionPPartner">pareja</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="CompanionP" value="familiar" id="companionPFamily" />
                <label class="form-check-label" for="companionPFamily">familiar</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="CompanionP" value="otro" id="companionPOther" />
                <label class="form-check-label" for="companionPOther">otro</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" asp-for="CompanionP" value="ninguno" id="companionPNone" />
                <label class="form-check-label" for="companionPNone">ninguno</label>
            </div>
        </div>
    </div>
</div>

<!-- FILA 5: Liquido meconial (separado segun formulario) -->

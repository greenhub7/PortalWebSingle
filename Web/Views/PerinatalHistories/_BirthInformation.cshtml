@model Domain.BirthInformation

<!-- FILA 1: PARTO/ABORTO | FECHA INGRESO | CONSULTAS PRENAT. | CARNE -->
<div class="row">
    <div class="col-md-2">
        <div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="BirthType" value="1" id="birthTypeParto" @(((int?)Model?.BirthType) == 1 ? "checked" : "") />
                <label class="form-check-label" for="birthTypeParto">Parto</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="BirthType" value="2" id="birthTypeAborto" @(((int?)Model?.BirthType) == 2 ? "checked" : "") />
                <label class="form-check-label" for="birthTypeAborto">Aborto</label>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <label>FECHA DE INGRESO</label>
        <input asp-for="AdmissionDate" type="date" class="form-control form-control-sm" />
    </div>
    <div class="col-md-2">
        <label>CONSULTAS PRENAT. total</label>
        <input asp-for="PrenatalConsultationsTotal" type="number" class="form-control form-control-sm" min="0" style="width: 70px;" />
    </div>
    <div class="col-md-2">
        <label>CARNE</label>
        <div>
            <div class="form-check form-check-inline">
                <input class="form-check-input bool-radio" type="radio" asp-for="HasPrenatalCard" value="false" id="prenatalCardNo" />
                <label class="form-check-label" for="prenatalCardNo">No</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input bool-radio" type="radio" asp-for="HasPrenatalCard" value="true" id="prenatalCardYes" />
                <label class="form-check-label" for="prenatalCardYes">Si</label>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <label>EDAD GEST. 1ra CONSULTA</label>
        <div class="input-group input-group-sm">
            <input asp-for="FirstConsultationGestationalAgeWeeks" type="number" class="form-control" min="0" max="42" style="width: 60px;" />
            <div class="input-group-append">
                <span class="input-group-text">semanas</span>
            </div>
        </div>
    </div>
</div>

<!-- FILA 2: HOSPITALIZ. EMBARAZO | CORTICOIDES | INICIO -->
<div class="row mt-2">
    <div class="col-md-3">
        <label>HOSPITALIZ. en EMBARAZO</label>
        <div class="d-flex align-items-center">
            <div class="form-check form-check-inline">
                <input class="form-check-input bool-radio" type="radio" asp-for="HospitalizedDuringPregnancy" value="false" id="hospitalizedNo" />
                <label class="form-check-label" for="hospitalizedNo">No</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input bool-radio" type="radio" asp-for="HospitalizedDuringPregnancy" value="true" id="hospitalizedYes" />
                <label class="form-check-label" for="hospitalizedYes">Si</label>
            </div>
            <div class="input-group input-group-sm ml-2">
                <input asp-for="HospitalizationDays" type="number" class="form-control" min="0" max="280" style="width: 60px;" />
                <div class="input-group-append">
                    <span class="input-group-text">dias</span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-5">
        <label>CORTICOIDES ANTENATALES</label>
        <div class="d-flex align-items-center flex-wrap">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="CorticosteroidsStatus" value="complete" id="corticosteroidsComplete" @(Model?.CorticosteroidsComplete == true ? "checked" : "") />
                <label class="form-check-label" for="corticosteroidsComplete">completo</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="CorticosteroidsStatus" value="incomplete" id="corticosteroidsIncomplete" @(Model?.CorticosteroidsIncomplete == true ? "checked" : "") />
                <label class="form-check-label" for="corticosteroidsIncomplete">incompl.</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="CorticosteroidsStatus" value="none" id="corticosteroidsNone" @(Model?.CorticosteroidsNone == true ? "checked" : "") />
                <label class="form-check-label" for="corticosteroidsNone">ninguna</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="CorticosteroidsStatus" value="na" id="corticosteroidsNA" @(Model?.CorticosteroidsNA == true ? "checked" : "") />
                <label class="form-check-label" for="corticosteroidsNA">n/c</label>
            </div>
            <div class="input-group input-group-sm ml-2" style="width: auto;">
                <input asp-for="CorticosteroidsStartWeek" type="number" class="form-control" min="20" max="42" style="width: 50px;" placeholder="" />
                <div class="input-group-append">
                    <span class="input-group-text">sem. inicio</span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <label>INICIO</label>
        <div class="d-flex">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="LaborOnset" value="spontaneous" id="spontaneousOnset" @(Model?.SpontaneousOnset == true ? "checked" : "") />
                <label class="form-check-label" for="spontaneousOnset">espontaneo</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="LaborOnset" value="induced" id="inducedOnset" @(Model?.InducedOnset == true ? "checked" : "") />
                <label class="form-check-label" for="inducedOnset">inducido</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="LaborOnset" value="elective_cesarean" id="electiveCesareanOnset" @(Model?.ElectiveCesareanOnset == true ? "checked" : "") />
                <label class="form-check-label" for="electiveCesareanOnset">cesar. elect.</label>
            </div>
        </div>
    </div>
</div>

<!-- FILA 3: ROTURA DE MEMBRANAS ANTEPARTO -->
<div class="row mt-2">
    <div class="col-md-12">
        <div class="form-group" style="background-color: #fff3cd; padding: 10px; border-radius: 4px; border-left: 4px solid #ffc107; margin-bottom: 0;">
            <label class="font-weight-bold">ROTURA DE MEMBRANAS ANTEPARTO</label>
            <div class="row align-items-end">
                <div class="col-md-1">
                    <div class="d-flex">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="MembraneRupture" value="false" id="membraneRuptureNo" @(Model?.MembraneRupture == false ? "checked" : "") />
                            <label class="form-check-label" for="membraneRuptureNo">no</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-1">
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="MembraneRupture" value="true" id="membraneRuptureYes" @(Model?.MembraneRupture == true ? "checked" : "") />
                        <label class="form-check-label" for="membraneRuptureYes">si</label>
                    </div>
                </div>
                <div class="col-md-2">
                    <label class="small">Fecha</label>
                    <input asp-for="MembraneRuptureDate" type="date" class="form-control form-control-sm" />
                </div>
              
                <div class="col-md-2">
                    <label class="small">Hora</label>
                    <input asp-for="MembraneRuptureTime" type="time" class="form-control form-control-sm" />
                </div>
                <div class="col-md-1">
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" name="MembraneRuptureBefore37Weeks" id="membraneRuptureBefore37Weeks"
                               @(Model?.MembraneRuptureBefore37Weeks == true ? "checked" : "") />
                        <label class="form-check-label" for="membraneRuptureBefore37Weeks">&lt;37 sem</label>
                    </div>
                </div>
                <div class="col-md-1">
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" name="MembraneRuptureMoreThan18Hours" id="membraneRuptureMoreThan18Hours"
                               @(Model?.MembraneRuptureMoreThan18Hours == true ? "checked" : "") />
                        <label class="form-check-label" for="membraneRuptureMoreThan18Hours">&ge;18 hs.</label>
                    </div>
                </div>
                <div class="col-md-2">
                    <label class="small">temp. &ge; 38C</label>
                    <div class="d-flex align-items-center">
                        <div class="form-check mr-2">
                            <input class="form-check-input" type="checkbox" name="FeverDuringLabor" id="feverDuringLabor"
                                   @(Model?.FeverDuringLabor == true ? "checked" : "") />
                        </div>
                        <input type="number" step="0.1" class="form-control form-control-sm" id="FeverTemperature" name="FeverTemperature" min="38" max="42" style="width: 70px;" placeholder="C" />
                    </div>
                </div>
      
            </div>
        </div>
    </div>
</div>

<!-- FILA 4: EDAD GEST. al parto | PRESENTACION/SITUACION | TAMANO FETAL | ACOMPANANTE TDP -->

        </div>
    </div>
    <div class="col-md-3">
        <label>LIQUIDO MECONIAL</label>
        <div class="d-flex">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="MeconiumStainedLiquor" value="1" id="meconiumStainedLiquorYes" @(((int?)Model?.MeconiumStainedLiquor) == 1 ? "checked" : "") />
                <label class="form-check-label" for="meconiumStainedLiquorYes">si</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="MeconiumStainedLiquor" value="2" id="meconiumStainedLiquorNo" @(((int?)Model?.MeconiumStainedLiquor) == 2 ? "checked" : "") />
                <label class="form-check-label" for="meconiumStainedLiquorNo">no</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="MeconiumStainedLiquor" value="3" id="meconiumStainedLiquorNc" @(((int?)Model?.MeconiumStainedLiquor) == 3 ? "checked" : "") />

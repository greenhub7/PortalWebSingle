<!-- ============ SECCION NACIMIENTO ============ -->
<div class="row mt-3">
    <div class="col-md-12">
        <div class="form-group" style="background-color: #e8f4fd; padding: 10px; border-radius: 4px; border-left: 4px solid #0d6efd;">
            <label class="font-weight-bold" style="font-size: 12px;">NACIMIENTO</label>
            <div class="row">
                <!-- VIVO/MUERTO -->
                <div class="col-md-2">
                    <div class="d-flex flex-column">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="BirthStatus" value="live" id="birthStatusLive" @(Model?.IsLiveBirth == true ? "checked" : "") />
                            <label class="form-check-label" for="birthStatusLive">VIVO</label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input conditional-trigger" data-target="deadBirthOptions" data-show-on-value="dead" type="radio" name="BirthStatus" value="dead" id="birthStatusDead" @(Model?.IsLiveBirth == false ? "checked" : "") />
                            <label class="form-check-label" for="birthStatusDead">MUERTO</label>
                        </div>
                        <div id="deadBirthOptions" class="ml-3" style="display: @(Model?.IsLiveBirth == false ? "block" : "none");">
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="DeadBirthTiming" value="anteparto" id="deadAnteparto" @(Model?.DeadBirthBeforeLabor == true ? "checked" : "") />
                                <label class="form-check-label small" for="deadAnteparto">anteparto</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="DeadBirthTiming" value="parto" id="deadParto" @(Model?.DeadBirthDuringLabor == true ? "checked" : "") />
                                <label class="form-check-label small" for="deadParto">parto</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="DeadBirthTiming" value="ignora" id="deadIgnora" @(Model?.DeadBirthDuringLaborUnknown == true ? "checked" : "") />
                                <label class="form-check-label small" for="deadIgnora">ignora momento</label>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- HORA y FECHA -->
                <div class="col-md-2">
                    <div class="row">
                        <div class="col-6">
                            <label class="small">hora</label>
                            <input type="time" class="form-control form-control-sm" id="BirthTime" name="BirthTime" value="@(Model?.BirthTime)" />
                        </div>
                        <div class="col-6">
                            <label class="small">fecha</label>
                            <input type="date" class="form-control form-control-sm" id="BirthDate" name="BirthDate" value="@(Model?.BirthDate?.ToString("yyyy-MM-dd"))" />
                        </div>
                    </div>
                </div>
                <!-- MULTIPLE orden -->
                <div class="col-md-2">
                    <label class="small">MULTIPLE órden</label>
                    <div class="d-flex align-items-center">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="MultipleBirthStatus" value="false" id="multipleNo" @(Model?.MultipleBirth == false ? "checked" : "") />
                            <label class="form-check-label" for="multipleNo">no</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="MultipleBirthStatus" value="true" id="multipleYes" @(Model?.MultipleBirth == true ? "checked" : "") />
                            <label class="form-check-label" for="multipleYes">si</label>
                        </div>
                        <input asp-for="BirthOrder" type="text" class="form-control form-control-sm ml-1" style="width: 50px;" placeholder="" />
                    </div>
                </div>
                <!-- TERMINACION -->
                <div class="col-md-2">
                    <label class="small">TERMINACION</label>
                    <div class="d-flex flex-wrap">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="TerminationType" value="spontaneous" id="termSpontaneous" @(Model?.SpontaneousBirth == true ? "checked" : "") />
                            <label class="form-check-label" for="termSpontaneous">Espont</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="TerminationType" value="cesarean" id="termCesarean" @(Model?.CesareanBirth == true ? "checked" : "") />
                            <label class="form-check-label" for="termCesarean">Cesárea</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="TerminationType" value="forceps" id="termForceps" @(Model?.ForcepsBirth == true ? "checked" : "") />
                            <label class="form-check-label" for="termForceps">Forceps</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="TerminationType" value="vacuum" id="termVacuum" @(Model?.VacuumBirth == true ? "checked" : "") />
                            <label class="form-check-label" for="termVacuum">Vacuum</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="TerminationType" value="other" id="termOther" @(Model?.OtherTermination == true ? "checked" : "") />
                            <label class="form-check-label" for="termOther">Otra</label>
                        </div>
                    </div>
                </div>
                <!-- INDICACION PRINCIPAL -->
                <div class="col-md-2">
                    <label class="small">INDICACION PRINCIPAL DE INDUCCION O PARTO OPERATORIO</label>
                    <textarea asp-for="MainIndicationForInductionOrOperativeDelivery" class="form-control form-control-sm" rows="2" style="font-size: 10px;"></textarea>
                </div>
                <!-- CODIGOS -->
                <div class="col-md-2">
                    <div class="row">
                        <div class="col-6">
                            <label class="small">INDUC.</label>
                            <input asp-for="InductionCode" type="text" class="form-control form-control-sm" placeholder="código" />
                        </div>
                        <div class="col-6">
                            <label class="small">OPER.</label>
                            <input asp-for="OperativeCode" type="text" class="form-control form-control-sm" placeholder="código" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- FILA 2 NACIMIENTO: POSICION, EPISIOTOMIA, DESGARROS, OCITOCICOS, PLACENTA, LIGADURA, MEDICACION -->

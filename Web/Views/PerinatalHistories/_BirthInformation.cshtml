        <div class="d-flex">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="PartogramDetails" value="false" id="partogramDetailsNo" @(Model?.PartogramDetails == false ? "checked" : "") />
                <label class="form-check-label" for="partogramDetailsNo">no</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="PartogramDetails" value="true" id="partogramDetailsYes" @(Model?.PartogramDetails == true ? "checked" : "") />

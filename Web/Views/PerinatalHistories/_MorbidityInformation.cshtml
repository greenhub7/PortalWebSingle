            @* Sub-opciones de hemorragia en 3 columnas *@
            <div class="row mt-2">
                @* Primera columna - 1er trimestre *@
                <div class="col-4">
                    <div class="d-flex align-items-center mb-1">
                        <span class="mr-1 small" style="min-width: 80px;">Post aborto</span>

@* SolMed 2026 - Morbidity Information with nullable radios *@
@model Domain.MorbidityInformation

@* Helper function for nullable radio group *@
@functions {
    string GetRadioChecked(string fieldValue, string optionValue) {
        return fieldValue == optionValue ? "checked" : "";
    }
}


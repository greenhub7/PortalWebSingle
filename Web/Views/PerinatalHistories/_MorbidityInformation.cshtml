    @* HEMORRAGIA *@
    <div class="col-md-4">
        <div class="form-group">
            <label class="font-weight-bold">HEMORRAGIA</label>

            @* Trimestres *@
            <div class="row mt-2">
                <div class="col-4">
                    <div class="d-flex flex-column">
                        <span class="font-weight-bold small">1<sup>er</sup> TRIM</span>
